    "babel-preset-react": "*",
    "babel-preset-stage-1": "*",
    "babel-register": "*",
    "css-loader": "^0.28.7",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-config-standard": "^7.0.0",
