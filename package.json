{
  "name": "HelloKoa2",
  "version": "0.1.0",
  "description": "a koa2 project",
  "private": true,
  "scripts": {
    "init_sql": "node ./init/index.js",
    "dev": "npm run dev_server & npm run dev_react",
    "dev_server": "nodemon --harmony ./server/app",
    "dev_static": "webpack --watch --config ./static/build/webpack.dev.config.js",
    "start_static": "webpack --config ./static/build/webpack.prod.config.js",
    "start_server": "node ./server/app.js",
    "start": "npm run start_static & npm run start_server"
  },
  "dependencies": {
    "ejs": "^2.5.5",
    "jade": "^1.11.0",
    "koa": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-onerror": "*",
    "koa-router": "^7.0.0",
    "koa-static": "^2.0.0",
    "koa-views": "^5.2.0",
    "path": "*",
    "xtemplate": "^4.2.4",
    "xtpl": "^3.1.9"
  },
  "devDependencies": {
    "babel": "*",
    "babel-cli": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-add-module-exports": "*",
    "babel-plugin-dev-expression": "*",
    "babel-preset-es2015": "*",
    "babel-preset-es2015-loose": "*",
    "babel-preset-es2015-loose-native-modules": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-1": "*",
    "babel-register": "*",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^2.1.1",
    "glob": "*",
    "jsx-loader": "*",
    "react": "*",
    "react-dom": "*",
    "react-router": "*",
    "webpack": "^1.14.0"
  }
}
