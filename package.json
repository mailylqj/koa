{
  "name": "HelloKoa2",
  "version": "0.1.0",
  "description": "a koa2 project",
  "private": true,
  "scripts": {
    "init_sql": "node ./init/index.js",
    "dev": "npm run dev_server && npm run dev_static",
    "dev_server": "nodemon ./server/app.js",
    "dev_static": "webpack --watch --config ./static/build/webpack.dev.config.js",
    "start_static": "webpack --config ./static/build/webpack.prod.config.js",
    "start_server": "node ./server/app.js",
    "start": "npm run start_static & npm run start_server",
    "test_server": "nodemon ./test/app.js",
    "test": "npm run test_server",
    "fast_dev": "webpack-dev-server --inline --hot --config ./static/build/webpack.dev.config.js"
  },
  "dependencies": {
    "ejs": "^2.5.5",
    "highcharts": "^5.0.14",
