    "koa-convert": "^1.2.0",
    "koa-onerror": "^3.1.0",
    "koa-route": "^3.2.0",
    "koa-router": "^7.0.0",
    "koa-static": "^2.0.0",
    "koa-views": "^5.2.0",
    "koa-websocket": "^4.0.0",
    "path": "^0.12.7",
    "react-websocket": "^1.1.7",
    "ws": "^3.1.0",
    "xtemplate": "^4.2.4",
    "xtpl": "^3.1.9"
  },
  "devDependencies": {
    "axios": "^0.16.2",
